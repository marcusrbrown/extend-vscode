{
  "name": "vscode-bash",
  "displayName": "Bash",
  "description": "Full language support for the Bourne Again SHell (Bash)",
  "version": "0.1.0",
  "author": "Marcus R. Brown <contact@marcusrbrown.com>",
  "publisher": "marcusrbrown",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcusrbrown/vscode-bash"
  },
  "bugs": {
    "url": "https://github.com/marcusrbrown/vscode-bash/issues"
  },
  "packageManager": "yarn@3.3.0",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:shellscript"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "yarn compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn compile && yarn lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "8.0.0",
    "@types/jest": "29.2.4",
    "@types/node": "18.x",
    "@types/prettier": "^2",
    "@types/vscode": "1.74.0",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "glob": "8.0.3",
    "jest": "29.3.1",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
    "typescript": "4.9.4"
  }
}
