{
  "name": "extend-vscode",
  "displayName": "Toolkit for building VS Code extensions.",
  "description": "Toolkit for building VS Code extensions.",
  "version": "0.1.0",
  "author": "Marcus R. Brown <contact@marcusrbrown.com>",
  "publisher": "marcusrbrown",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcusrbrown/extend-vscode"
  },
  "bugs": {
    "url": "https://github.com/marcusrbrown/extend-vscode/issues"
  },
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:shellscript"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "pnpm compile",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "compile": "tsc -p ./",
    "lint": "eslint .",
    "watch": "tsc -watch -p ./",
    "test": "jest"
  },
  "devDependencies": {
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.10",
    "@types/prettier": "3.0.0",
    "@types/vscode": "1.91.0",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "7.16.0",
    "@vscode/test-electron": "2.4.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "glob": "10.4.5",
    "jest": "29.7.0",
    "prettier": "3.3.2",
    "ts-jest": "29.2.0",
    "typescript": "5.4.5"
  }
}
