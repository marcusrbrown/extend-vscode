{
  "name": "vscode-bash",
  "displayName": "Bash",
  "description": "Full language support for the Bourne Again SHell (Bash)",
  "version": "0.1.0",
  "author": "Marcus R. Brown <contact@marcusrbrown.com>",
  "publisher": "marcusrbrown",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcusrbrown/vscode-bash"
  },
  "bugs": {
    "url": "https://github.com/marcusrbrown/vscode-bash/issues"
  },
  "packageManager": "pnpm@8.15.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:shellscript"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "pnpm compile",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "compile": "tsc -p ./",
    "lint": "eslint .",
    "watch": "tsc -watch -p ./",
    "test": "jest"
  },
  "devDependencies": {
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.16",
    "@types/prettier": "2.7.3",
    "@types/vscode": "1.86.0",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "@vscode/test-electron": "2.3.9",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "glob": "10.3.10",
    "jest": "29.7.0",
    "prettier": "3.2.2",
    "ts-jest": "29.1.2",
    "typescript": "5.3.3"
  }
}
