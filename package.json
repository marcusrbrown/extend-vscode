{
  "name": "vscode-bash",
  "displayName": "Bash",
  "description": "Full language support for the Bourne Again SHell (Bash)",
  "version": "0.1.0",
  "author": "Marcus R. Brown <contact@marcusrbrown.com>",
  "publisher": "marcusrbrown",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcusrbrown/vscode-bash"
  },
  "bugs": {
    "url": "https://github.com/marcusrbrown/vscode-bash/issues"
  },
  "packageManager": "pnpm@9.3.0",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:shellscript"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "pnpm compile",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "compile": "tsc -p ./",
    "lint": "eslint .",
    "watch": "tsc -watch -p ./",
    "test": "jest"
  },
  "devDependencies": {
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.2",
    "@types/prettier": "3.0.0",
    "@types/vscode": "1.90.0",
    "@typescript-eslint/eslint-plugin": "7.13.0",
    "@typescript-eslint/parser": "7.13.0",
    "@vscode/test-electron": "2.4.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "glob": "10.4.1",
    "jest": "29.7.0",
    "prettier": "3.3.1",
    "ts-jest": "29.1.4",
    "typescript": "5.4.5"
  }
}
